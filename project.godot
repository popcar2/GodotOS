; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GodotOS"
run/main_scene="res://Scenes/Main/MainScene.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/low_processor_mode=true
config/icon="res://icon.svg"

[autoload]

DefaultValues="res://Scenes/Autoloads/default_values.tscn"
HotkeyManager="res://Scenes/Autoloads/hotkey_manager.tscn"

[file_customization]

folder_colors={
"res://Scenes/": "blue",
"res://Scenes/Misc/": "gray"
}

[filesystem]

import/blender/enabled=false

[input]

kill_all_windows={
"deadzone": 0.5,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":true,"ctrl_pressed":true,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":4194312,"key_label":0,"unicode":0,"echo":false,"script":null)
]
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
